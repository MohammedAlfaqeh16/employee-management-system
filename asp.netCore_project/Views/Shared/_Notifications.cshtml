<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/toastr.js/toastr.min.js"></script>
@if (TempData["SuccessData"] != null)
{

    <script type="text/javascript">
        toastr["success"]('@TempData["SuccessData"]');
    </script>

    <div class="alert alert-info text-center mt-4">
        @TempData["SuccessData"]
    </div>
}
@if (TempData["SuccessDat"] != null)
{
    <script type="text/javascript">
      

        toastr.options = {
          "closeButton": true,
          "debug": false,
          "newestOnTop": true,
          "progressBar": false,
          "positionClass": "toast-top-center",
          "preventDuplicates": true,
          "showDuration": "300",
          "hideDuration": "1000",
          "timeOut": "5000",
          "extendedTimeOut": "1000",
          "showEasing": "swing",
          "hideEasing": "swing",
          "showMethod": "fadeIn",
          "hideMethod": "fadeOut"

        }
         toastr["error"]('@TempData["SuccessDat"]');
    </script>

    <div class="alert alert-danger text-center text-danger mt-4">
        @TempData["SuccessDat"]
    </div>
}